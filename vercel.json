{
  "version": 2,
  "builds": [
    {
      "src": "server/index-gmail.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/index-gmail.js"
    }
  ],
  "env": {
    "GMAIL_USER": "@gmail_user",
    "GMAIL_APP_PASSWORD": "@gmail_app_password",
    "ADMIN_EMAIL": "@admin_email",
    "FRONTEND_URL": "@frontend_url"
  }
}